{
  "name": "xvideos.js",
  "version": "1.0.0",
  "description": "powerful xvideos wrapper based on typescript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/OSintt/xvideos.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "dev": "nodemon",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "outDir": "dist",
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "keywords": [
    "porn",
    "xvideos",
    "scraper",
    "scraping",
    "web scraping",
    "puppeteer",
    "cheerio",
    "typescript",
    "web-scraper",
    "library"
  ],
  "author": "OSintt",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "chai": "^5.1.1",
    "esbuild": "^0.23.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0",
    "ts-jest": "^29.2.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "1.0.0-rc.12",
    "puppeteer": "^22.13.1"
  }
}
